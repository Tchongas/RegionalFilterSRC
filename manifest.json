{
  "manifest_version": 3,
  "name": "SRC Country Filter",
  "description": "An extension that can filter runs by country",
  "version": "1.1",
  "permissions": ["activeTab"],
  "host_permissions": ["https://www.speedrun.com/*"],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.speedrun.com/*"],
      "js": ["scripts/content.js"]
    }
  ]
}